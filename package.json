{
  "name": "bitcoinprices",
  "version": "1.0.0",
  "description": "See the most traded commodities priced in BTC.",
  "scripts": {
    "start": "npm-run-all sass:build vercel:prev",
    "prod": "npm-run-all sass:build vercel:prod",
    "server": "npm-run-all --parallel sass:dev vercel:dev",
    "vercel:prev": "vercel",
    "vercel:prod": "vercel --prod",
    "vercel:dev": "vercel dev",
    "sass:dev": "sass --watch sass/:public/styles/",
    "sass:build": "sass sass/:public/styles/"
  },
  "keywords": [
    "bitcoin",
    "commodities",
    "commodities prices"
  ],
  "author": "Wemerson Jesus Silva",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/wejesuss/bitcoinprices"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.34.1",
    "vercel": "^23.0.0"
  },
  "dependencies": {
    "request": "^2.88.2"
  }
}
